<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ver_logros"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.laudy.francesa1.app.appfrancesa1.VerLogros"
    android:background="@drawable/fondologros">

    <Button
        android:id="@+id/btnSalir"
        android:text="@string/btnSalir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@android:style/Widget.Button"
        tools:background="#2fc8d9"
        android:textColor="#330099"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        />

    <Button
        android:id="@+id/btnVerLogros"
        android:text="@string/btnVerLogros"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@android:style/Widget.Button"
        tools:background="@android:color/holo_blue_light"
        android:textColor="#330099"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        />

    <Button
        android:id="@+id/btnVerPerfil"
        android:text="@string/btnVerPerfil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@android:style/Widget.Button"
        tools:background="@android:color/holo_blue_light"
        android:textColor="#330099"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/btnVerDossiers"
        />

    <Button
        android:id="@+id/btnVerDossiers"
        android:text="@string/btnVerDossiers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@android:style/Widget.Button"
        tools:background="#2fc8d9"
        android:textColor="#330099"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/btnVerLogros"
        />

    <TextView
        android:id="@+id/lblTituloLogros"
        android:text="@string/lblTituloLogros"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="22sp"
        android:textColor="#0066CC"
        android:layout_marginTop="32dp"
        android:layout_below="@+id/btnVerLogros"
        android:layout_toStartOf="@+id/btnVerDossiers"
        />

    <TextView
        android:id="@+id/lblPuntaje"
        android:text="@string/lblPuntaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_alignBaseline="@+id/lblTituloLogros"
        android:layout_alignBottom="@+id/lblTituloLogros"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="50dp"
        />

    <TextView
        android:id="@+id/lblPuntajeFalta"
        android:text="@string/lblPuntajeFalta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_below="@+id/lblTituloLogros"
        android:layout_alignStart="@+id/lblPuntaje"
        android:layout_marginTop="10dp"
        />

    <TextView
        android:id="@+id/lblPuntaje2"
        android:text="@string/lblPuntaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_below="@+id/lblPuntajeFalta"
        android:layout_alignStart="@+id/lblPuntajeFalta"
        android:layout_marginTop="40dp" />

    <TextView
        android:id="@+id/lblPuntajeFalta2"
        android:text="@string/lblPuntajeFalta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_below="@+id/lblPuntaje2"
        android:layout_alignStart="@+id/lblPuntaje2"
        android:layout_marginTop="10dp"
        />

    <TextView
        android:id="@+id/lblPuntaje3"
        android:text="@string/lblPuntaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_below="@+id/lblPuntajeFalta"
        android:layout_alignStart="@+id/lblPuntajeFalta"
        android:layout_marginTop="130dp" />

    <TextView
        android:id="@+id/lblPuntajeFalta3"
        android:text="@string/lblPuntajeFalta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_below="@+id/lblPuntaje2"
        android:layout_alignStart="@+id/lblPuntaje2"
        android:layout_marginTop="100dp"
        />

    <TextView
        android:id="@+id/lblPuntaje4"
        android:text="@string/lblPuntaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_below="@+id/lblPuntajeFalta"
        android:layout_alignStart="@+id/lblPuntajeFalta"
        android:layout_marginTop="210dp" />

    <TextView
        android:id="@+id/lblPuntajeFalta4"
        android:text="@string/lblPuntajeFalta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_below="@+id/lblPuntaje2"
        android:layout_alignStart="@+id/lblPuntaje2"
        android:layout_marginTop="180dp"
        />

    <TextView
        android:id="@+id/lblPuntaje5"
        android:text="@string/lblPuntaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_below="@+id/lblPuntajeFalta"
        android:layout_alignStart="@+id/lblPuntajeFalta"
        android:layout_marginTop="290dp" />

    <TextView
        android:id="@+id/lblPuntajeFalta5"
        android:text="@string/lblPuntajeFalta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textSize="12sp"
        android:textColor="#0066CC"
        android:layout_below="@+id/lblPuntaje2"
        android:layout_alignStart="@+id/lblPuntaje2"
        android:layout_marginTop="260dp"
        />

</RelativeLayout>
